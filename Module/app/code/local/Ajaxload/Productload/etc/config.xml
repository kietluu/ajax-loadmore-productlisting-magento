<?xml version="1.0"?>
<config>
    <modules>
        <AjaxLoad_Productload>
            <version>0.1.0</version>
        </AjaxLoad_Productload>
    </modules>
    <global>
        <models>
            <magento_productload>
                <class>Magento_Productload_Model</class>
            </magento_productload>
        </models>
        <helpers>
            <ajaxload_productload>
                <class>Ajaxload_Productload_Helper</class>
            </ajaxload_productload>
        </helpers>
    </global>
    <adminhtml>
        <acl>
            <resources>
                <all>
                    <title>Allow Everything</title>
                </all>
                <admin>
                    <children>
                        <system>
                            <children>
                                <config>
                                    <children>
                                       <ajaxload_productload>
                                           <title>Product Load More Ajax</title>
                                       </ajaxload_productload>
                                    </children>
                                </config>
                            </children>
                        </system>
                    </children>
                </admin>
            </resources>
        </acl>
    </adminhtml>
</config>